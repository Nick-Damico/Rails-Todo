<table class="table table-dark list-table">
  <thead>
    <tr>
      <th scope="col">Index</th>
      <th scope="col">List Name</th>
      <th scope="col">List Link</th>
    </tr>
  </thead>
  <tbody class="table-body">
    <% lists.each do |list| %>
      <tr class="row-<%= list.id %>">
        <%= render partial: 'todo-item', locals: { list: list } %>
      </tr>
    <% end %>
  </tbody>
</table>
